{
  
  "name": "",
  "description": "",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": [
            "js/jquery.min.js",
            "js/content_script.js"
            ],
      "run_at":"document_idle"
    }],
   "background":{},
  "permissions": [
       "storage",
       "tabs",
       "http://*/*",
       "https://*/*"
      
  ],
  "options_page": "html/option.html",
  "browser_action": {
        "default_icon": "icon/icon_16.png",
        "default_popup": "html/popup.html"
    },
  "icons": {
    "16": "icon/icon_16.png",
    "48": "icon/icon_48.png",
    "64": "icon/icon_64.png",
    "128": "icon/icon_128.png"
  },
  "manifest_version": 1
}
